{
  "name": "orbiter",
  "version": "0.1.0",
  "description": "Orbiter is a Electron App based on Rocket.Chat\"",
  "main": "main.js",
  "repository": "https://github.com/cazetto/orbiter.git",
  "author": "Andr√© <cazetto.andre@gmail.com>",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "test": "yarn lint",
    "lint": "echo"
  },
  "devDependencies": {
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^7.0.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  }
}
